{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "DacoidChat",
  "colors": {
    "primary": "#0075fd",
    "light": "#0075fd",
    "dark": "#0075fd"
  },
  "favicon": "/favicon.png",
  "fonts":{
    "body": {
      "family": "Inter"
    },
    "heading": {
      "family": "Gabarito"
    }
  },
  "appearance": {
    "default": "light"
  },
  "background": {
    "decoration": "gradient"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "pages": [
          "index",
          "docs/get-started",
          "docs/workspace",
          "docs/navigate",
          "docs/creating-your-first-chatbot",
          "docs/function-&-varaibles",
          "docs/knowledge-base",
          "docs/widget"
        ]
      },
      {
        "tab": "FAQ",
        "pages": [
          "docs/faq"
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Contact Us",
          "href": "mailto:support@dacoidchat.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png",
    "href": "https://dacoidchat.com/"
  },
  "navbar": {
    "links": [
      {
        "label": "Login",
        "href": "https://dacoidchat.com/login"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Start free trial",
      "href": "https://dacoidchat.com/register"
    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://www.linkedin.com/company/dacoid-digital/"
    }
  }
}
